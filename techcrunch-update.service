[Unit]
Description=TechCrunch AI RSS Daily Update
After=network.target

[Service]
Type=oneshot
User=www-data
WorkingDirectory=/path/to/techcrunch-ai-rss
Environment="PATH=/usr/bin:/usr/local/bin"
ExecStart=/usr/bin/python3 /path/to/techcrunch-ai-rss/scheduled_update.py
StandardOutput=journal
StandardError=journal
